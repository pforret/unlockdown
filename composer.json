{
    "name": "pforret/unlockdown",
    "version": "2.0.3",
    "description": "Import RSS feed or Wordpress XML to static Markdown pages",
    "keywords": [
        "pforret",
        "unlockdown"
    ],
    "homepage": "https://github.com/pforret/unlockdown",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "hjbdev/php-wordpress-xml-parser": "^1.0",
        "league/html-to-markdown": "^5.1",
        "nesbot/carbon": "^3.8",
        "simplepie/simplepie": "^1.8"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.18.6",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\Unlockdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\Unlockdown\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "setup-ci": "vendor/bin/rector setup-ci",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
